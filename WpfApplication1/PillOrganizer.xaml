<UserControl x:Class="WpfApplication1.PillOrganizer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:drt="clr-namespace:WpfApplication1"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="700" Loaded="PillOrganizer_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height=".2*" />
            <RowDefinition Height=".2*" />
            <RowDefinition Height=".2*" />
            <RowDefinition Height=".2*" />
            <RowDefinition Height=".2*" />
        </Grid.RowDefinitions>
        <drt:PillOrganizerNumberPicker Grid.Row="0" x:Name="Parlenol"></drt:PillOrganizerNumberPicker>
        <drt:PillOrganizerNumberPicker Grid.Row="1" x:Name="BRB"></drt:PillOrganizerNumberPicker>
        <drt:PillOrganizerNumberPicker Grid.Row="2" x:Name="Linophen"></drt:PillOrganizerNumberPicker>
        <drt:PillOrganizerNumberPicker Grid.Row="3" x:Name="Anaanx"></drt:PillOrganizerNumberPicker>
        <drt:PillOrganizerNumberPicker Grid.Row="4" x:Name="Cyclomeovan"></drt:PillOrganizerNumberPicker>
    </Grid>

</UserControl>
