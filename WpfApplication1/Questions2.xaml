        <StackPanel Grid.Row="10" Height="Auto" HorizontalAlignment="Left" Name="stackPanel2" VerticalAlignment="Top" Width="Auto" Grid.ColumnSpan="4" Grid.Column="1" Visibility="Hidden" Margin="0,17,0,0" Grid.RowSpan="2">
